{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "issuer",
    "output_descriptors"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "issuer": {
      "type": "object",
      "required": [
        "id",
        "name",
        "styles"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "styles": {
          "type": "object",
          "required": [],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "output_descriptors": {
      "type": "array",
      "additionalItems": false,
      "items": {
        "anyOf": [
          {
            "type": "object",
            "required": [
              "schema",
              "display",
              "styles"
            ],
            "properties": {
              "schema": {
                "type": "string",
                "format": "uri"
              },
              "display": {
                "type": "object",
                "required": [
                  "title",
                  "subtitle",
                  "description",
                  "properties"
                ],
                "properties": {
                  "title": {
                    "type": "object",
                    "required": [
                      "path",
                      "fallback"
                    ],
                    "properties": {
                      "path": {
                        "type": "array",
                        "additionalItems": false,
                        "items": {
                          "anyOf": [
                            {
                              "type": "string"
                            }
                          ]
                        }
                      },
                      "fallback": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "subtitle": {
                    "type": "object",
                    "required": [
                      "path",
                      "fallback"
                    ],
                    "properties": {
                      "path": {
                        "type": "array",
                        "additionalItems": false,
                        "items": {
                          "anyOf": [
                            {
                              "type": "string"
                            }
                          ]
                        }
                      },
                      "fallback": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "description": {
                    "type": "object",
                    "required": [
                      "text"
                    ],
                    "properties": {
                      "text": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "properties": {
                    "type": "array",
                    "additionalItems": false,
                    "items": {
                      "anyOf": [
                        {
                          "type": "object",
                          "required": [
                            "path",
                            "fallback",
                            "label"
                          ],
                          "properties": {
                            "path": {
                              "type": "array",
                              "additionalItems": false,
                              "items": {
                                "anyOf": [
                                  {
                                    "type": "string"
                                  }
                                ]
                              }
                            },
                            "fallback": {
                              "type": "string"
                            },
                            "label": {
                              "type": "string"
                            }
                          },
                          "additionalProperties": false
                        }
                      ]
                    }
                  }
                },
                "additionalProperties": false
              },
              "styles": {
                "type": "object",
                "required": [],
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          }
        ]
      }
    },
    "presentation_definition": {
      "type": "object",
      "required": [],
      "additionalProperties": false
    },
    "format": {}
  },
  "additionalProperties": false
}
